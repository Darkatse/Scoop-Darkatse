        [
            "Netch.exe",
            "Netch"
        ]
    ],
    "persist": [
        "data",
        "logging",
        "mode"
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/NetchX/Netch/releases/download/$version/Netch.zip"
            }
        },
        "hash": {
            "url": "https://github.com/NetchX/Netch/releases/tag/$version",
            "regex": "(?sm)$basename</td.*?$sha256"
        }
    }
